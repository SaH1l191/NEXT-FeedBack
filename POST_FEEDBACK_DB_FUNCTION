declare
  new_feedback_id bigint;    -- Declare a variable to store the new feedback's ID
begin
  
  insert into feedbacks (project_id, user_name, user_email, message,p_rating)
  values (p_project_id, p_user_name, p_user_email, p_message,p_rating)
  returning id into new_feedback_id;  -- Capture the ID of the newly inserted feedback

  -- Return the ID of the new feedback
  return new_feedback_id;
end;
 

USECASE : 
SELECT add_feedback(7, 'John filama', 'johnfialama@example.com', 'Great project, but could use some improvements!',4);
